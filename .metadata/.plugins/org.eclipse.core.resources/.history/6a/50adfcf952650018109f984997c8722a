package com.alisa.junit;

public class aa {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int N = 72316829;
		int result = 1;
        for(int i = 1; i <= N/2; i++){//Ò»°ë
            int sum = 0;
             for(int j = i; (j^2 )< (2*N -i + i^2); j++){
                 sum = sum + j ;
                 if(sum == N){
                     result = result + 1;                     
                 }else if(sum > N){
                     break;
                 }         
             }
        }    
        
        System.out.println(result);
	}

}
