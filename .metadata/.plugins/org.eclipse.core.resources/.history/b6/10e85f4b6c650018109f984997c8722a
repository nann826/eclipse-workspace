package com.alisa.junit;

public class aa {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int N = 15;//72316829;
		int result = 1;
        for(int i = 1; i <= N/2; i++){//Ò»°ë
            int sum = 0;
            int topvalue = 2*N - i + (i^2);
           for( int j = 1;j*j <= topvalue; j ++) {
        	   if((j * j+1) == topvalue ) {
        	   result = result + 1;    
        	   }
            }
    
        }    
        
        System.out.println(result);
	}

}
